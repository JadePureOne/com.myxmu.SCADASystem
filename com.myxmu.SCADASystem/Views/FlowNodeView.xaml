<UserControl
    x:Class="com.myxmu.SCADASystem.Views.FlowNodeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:com.myxmu.SCADASystem.Views"
    xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:userControls="clr-namespace:com.myxmu.SCADASystem.UserControls"
    xmlns:vm="clr-namespace:com.myxmu.SCADASystem.ViewModels"
    xmlns:wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
    d:DataContext="{d:DesignInstance vm:FlowNodeViewModel}"
    d:DesignHeight="800"
    d:DesignWidth="1200"
    FontFamily="Microsoft YaHei"
    TextElement.FontSize="16"
    TextElement.FontWeight="Regular"
    mc:Ignorable="d">
    <Grid>
        <wpf:WebView2 Source="http://127.0.0.1:1880/" />
    </Grid>
</UserControl>
